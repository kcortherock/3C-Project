{
  "name": "toolbox",
  "displayName": "toolbox",
  "description": "Awesomeness",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      
    {
      "command": "nestingTracker.showLevels",
      "title": "Calculate Nesting Levels"
    },
    {
      "command": "extension.totalComplexity",
      "title": "Get Total Complexity Score of file"
    },
    {
      "command": "extension.highlightLine",
      "title": "Highlight Line in Current File"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
